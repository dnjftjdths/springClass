<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.sinc.test">
	<select id="select"
			resultType="test" >
		SELECT 	ID, PWD, NAME
		FROM	SPRING_TEST_MEN_TBL
	</select>
	<insert id="insert"
			parameterType="test">
		INSERT INTO SPRING_TEST_MEN_TBL
		VALUES(#{id}, #{pwd}, #{name})
	</insert>
</mapper>  











